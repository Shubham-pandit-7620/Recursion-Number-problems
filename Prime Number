import java.util.Scanner;

public class Prime {
    public static boolean isprime(int num, int start, boolean f) {
        if (num == start) {
            return f;

        }
        if (num % start == 0 || num == 1) {
            return false;

        } else {
            f = true;
        }
        return isprime(num, start + 1, f);

    }

    public static void main(String[] args) {

        System.out.println(isprime(19, 2, true));
    }

}
